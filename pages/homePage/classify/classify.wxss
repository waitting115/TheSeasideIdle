.all {
  width: 100%;
  height: 100%;
  background: #f2f2f2;
  padding: 0;
  margin: 0;
}
.title {
  width: 100%;
  height: 100rpx;
  border: 1px solid #ccc;
  font-size: 80rpx;
  line-height: 100rpx;
  text-align: center;
  background: #fff;
  margin-bottom: 3%;
  font-family: serif;
}
.footer {
  width: 100%;
  height: auto;
  border-top: 1px solid #ccc;
  text-align: center;
  color: #8c8c8c;
}